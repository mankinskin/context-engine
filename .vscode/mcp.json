{
  "servers": {
    "docs-server": {
      "type": "stdio",
      "command": "cargo",
      "args": [
        "run",
        "--manifest-path",
        "${workspaceFolder}/tools/doc-viewer/Cargo.toml",
        "--",
        "--mcp"
      ],
      "env": {
        "DOCS_ROOT": "${workspaceFolder}"
      }
    }
  }
}
