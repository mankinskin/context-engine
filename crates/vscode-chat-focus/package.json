{
  "name": "chat-focus",
  "displayName": "Chat Focus",
  "description": "Provides commands to focus GitHub Copilot Chat",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "chat-focus.focusChat",
        "title": "Focus Copilot Chat"
      }
    ],
    "keybindings": [
      {
        "command": "chat-focus.focusChat",
        "key": "ctrl+alt+c",
        "mac": "cmd+alt+c"
      }
    ]
  },
  "scripts": {
    "package": "vsce package"
  }
}